<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:top="0px">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="#444444"/>
        </shape>
    </item>
    <item android:top="1dp" android:bottom="1dp">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="#3cb5b4"/>
        </shape>
    </item>
    <item android:top="2dp" android:bottom="2dp">
        <shape android:shape="rectangle">
            <gradient
               android:startColor="#39abaa"
               android:endColor="#2d8786"
               android:type="linear"
               android:angle="270"
               android:useLevel="false"/>
<!--            <gradient-->
<!--               android:startColor="#efefef"-->
<!--               android:endColor="#cfcfcf"-->
<!--               android:type="linear"-->
<!--               android:angle="270"-->
<!--               android:useLevel="false"/>-->
        </shape>
    </item>
</layer-list>
